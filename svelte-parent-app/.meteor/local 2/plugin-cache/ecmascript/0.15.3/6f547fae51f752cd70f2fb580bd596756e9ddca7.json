 = {\n    displayMessage: displayMessage,\n    alertMessage: alertMessage,\n    incrementCount: incrementCount // decrementCount,\n\n  };\n\n  if (message.origin === CHILD_URL) {\n    //sanity check\n    console.log(\"incoming message in parent MessageHandler from child!\");\n    console.log(message); //parse input\n\n    functionName = message.data.function;\n    args = message.data.args; //call appropriate function\n\n    if (functionAdapter[functionName]) {\n      functionAdapter[functionName](args);\n    } else {\n      throw new Error(\"invalid function name\");\n    }\n  } else if (message.origin === PARENT_URL) {\n    //sanity check\n    console.log(\"incoming message in parent MessageHandler from own URL\");\n  } else {\n    //permission denied\n    throw new Error(\"invalid parent URL, permission denied\");\n  }\n});","map":{"version":3,"sources":["imports/api/ChildToParentAPI.js"],"names":["PARENT_URL","CHILD_URL","module","link","v","incomingMessageText","count","displayMessage","args","console","log","messageText","set","alertMessage","alert","incrementCount","$count","decrementCount","window","addEventListener","message","functionAdapter","origin","functionName","data","function","Error"],"mappings":"AAAA,IAAIA,UAAJ,EAAeC,SAAf;AAAyBC,MAAM,CAACC,IAAP,CAAY,mBAAZ,EAAgC;AAACH,EAAAA,UAAU,EAAC,UAASI,CAAT,EAAW;AAACJ,IAAAA,UAAU,GAACI,CAAX;AAAa,GAArC;AAAsCH,EAAAA,SAAS,EAAC,UAASG,CAAT,EAAW;AAACH,IAAAA,SAAS,GAACG,CAAV;AAAY;AAAxE,CAAhC,EAA0G,CAA1G;AAA6G,IAAIC,mBAAJ,EAAwBC,KAAxB;AAA8BJ,MAAM,CAACC,IAAP,CAAY,qBAAZ,EAAkC;AAACE,EAAAA,mBAAmB,EAAC,UAASD,CAAT,EAAW;AAACC,IAAAA,mBAAmB,GAACD,CAApB;AAAsB,GAAvD;AAAwDE,EAAAA,KAAK,EAAC,UAASF,CAAT,EAAW;AAACE,IAAAA,KAAK,GAACF,CAAN;AAAQ;AAAlF,CAAlC,EAAsH,CAAtH;;AAGpK;AACA,SAASG,cAAT,CAAwBC,IAAxB,EAA8B;AAC5B;AACAC,EAAAA,OAAO,CAACC,GAAR,4DAAqEF,IAArE,QAF4B,CAI5B;;AACA,MAAI,OAAOA,IAAI,CAACG,WAAZ,KAA4B,QAAhC,EAA0C;AACxCN,IAAAA,mBAAmB,CAACO,GAApB,CAAwBJ,IAAI,CAACG,WAA7B;AACD;AACF,C,CAED;;;AACA,SAASE,YAAT,CAAsBL,IAAtB,EAA4B;AAC1B;AACAC,EA