ue,"sourceFileName":"packages/rdb:svelte-meteor-data/use-session.js"}},"code":"module.export({\n  default: () => useSession\n});\nlet Session;\nmodule.link(\"meteor/session\", {\n  Session(v) {\n    Session = v;\n  }\n\n}, 0);\nlet EJSON;\nmodule.link(\"meteor/ejson\", {\n  EJSON(v) {\n    EJSON = v;\n  }\n\n}, 1);\nlet nextId = 1;\n\nconst parse = serialized => serialized !== undefined && serialized !== 'undefined' ? EJSON.parse(serialized) : undefined;\n\nfunction useSession(key, defaultValue) {\n  if (arguments.length > 1) {\n    Session.setDefault(key, defaultValue);\n  }\n\n  return {\n    subscribe(set) {\n      Session._ensureKey(key);\n\n      const dep = Session.keyDeps[key];\n\n      if (Object.prototype.hasOwnProperty.call(Session.keys, key)) {\n        set(parse(Session.keys[key]));\n      }\n\n      const id = \"svelte-session-\".concat(nextId++);\n      dep._dependentsById[id] = {\n        _id: id,\n        invalidate: () => {\n          set(parse(Session.keys[key]));\n        }\n      };\n      return () => {\n        delete dep._dependentsById[id];\n      };\n    },\n\n    set(value) {\n      Session.set(key, value);\n    }\n\n  };\n}\n\n;","map":{"version":3,"sources":["packages/rdb:svelte-meteor-data/use-session.js"],"names":["module","export","default","useSession","Session","link","v","EJSON","nextId","parse","serialized","undefined","key","defaultValue","arguments","length","setDefault","subscribe","set","_ensureKey","dep","keyDeps","Object","prototype","hasOwnProperty","call","keys","id","_dependentsById","_id","invalidate","value"],"mappings":"AAAAA,MAAM,CAACC,MAAP,CAAc;AAACC,EAAAA,OAAO,EAAC,MAAIC;AAAb,CAAd;AAAwC,IAAIC,OAAJ;AAAYJ,MAAM,CAACK,IAAP,CAAY,gBAAZ,EAA6B;AAACD,EAAAA,OAAO,CAACE,CAAD,EAAG;AAACF,IAAAA,OAAO,GAACE,CAAR;AAAU;;AAAtB,CAA7B,EAAqD,CAArD;AAAwD,IAAIC,KAAJ;AAAUP,MAAM,CAACK,IAAP,CAAY,cAAZ,EAA2B;AAACE,EAAAA,KAAK,CAACD,CAAD,EAAG;AAACC,IAAAA,KAAK,GAACD,CAAN;AAAQ;;AAAlB,CAA3B,EAA+C,CAA/C;AAOtH,IAAIE,MAAM,GAAG,CAAb;;AAEA,MAAMC,KAAK,GAAGC